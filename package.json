  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "vercel-build": "vite build",
    "start": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\" \"api/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\" \"api/**/*.ts\"",
    "check-layout": "node scripts/check-layout.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:visual": "playwright test tests/visual --project=chromium",
    "test:e2e": "playwright test tests/e2e",
    "test:all": "playwright test",
    "test:ui": "playwright test --ui",
    "test:multimodal": "node scripts/test-multimodal.js",
    "test:rekognition": "node scripts/test-rekognition-endpoint.js",
    "test:pre-deploy": "npm run test:multimodal && npm run test:rekognition",
    "playwright:install": "playwright install chromium",
    "predeploy": "npm run test:pre-deploy && npm run check-layout",
    "guard:aws": "echo 'AWS configuration guard - checking for exposed secrets'",
    "guard:cross": "echo 'Cross-import guard placeholder - add specific checks as needed'",
    "build:mobile": "npm run build && npx cap sync",
    "build:ios": "npm run build:mobile && npx cap run ios",
    "build:android": "npm run build:mobile && npx cap run android",
    "build:all": "npm run build:mobile && echo 'Mobile build complete - ready for iOS and Android'",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,json,md}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "@vercel/node": {
      "path-to-regexp": "^8.2.0",
      "undici": "^7.0.0"
    }
  }
}
